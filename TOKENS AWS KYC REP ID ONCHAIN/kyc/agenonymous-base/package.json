{
  "name": "accredefi-kyc-system",
  "version": "1.0.0",
  "description": "AccreDeFi KYC and Compliance Verification System - 98% complete identity verification infrastructure for WrapPliant Protocol",
  "main": "accredefi-api-server.js",
  "scripts": {
    "start": "node accredefi-api-server.js",
    "dev": "nodemon accredefi-api-server.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint *.js",
    "lint:fix": "eslint *.js --fix",
    "db:migrate": "node scripts/migrate-database.js",
    "db:seed": "node scripts/seed-database.js",
    "cleanup": "node scripts/cleanup-expired.js",
    "validate": "node scripts/validate-system.js"
  },
  "keywords": [
    "kyc",
    "compliance",
    "accredefi",
    "wrappliant",
    "ton",
    "blockchain",
    "identity-verification",
    "biometric",
    "document-processing",
    "regulatory-compliance"
  ],
  "author": "AccreDeFi Team",
  "license": "BUSL-1.1",
  "dependencies": {
    "@aws-sdk/client-textract": "^3.450.0",
    "@ton/ton": "^13.9.0",
    "@ton/crypto": "^3.2.0",
    "express": "^4.18.2",
    "multer": "^1.4.5-lts.1",
    "sharp": "^0.32.6",
    "crypto": "^1.0.1",
    "axios": "^1.6.0",
    "winston": "^3.11.0",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "express-rate-limit": "^7.1.5",
    "compression": "^1.7.4",
    "pg": "^8.11.3",
    "bcrypt": "^5.1.1",
    "jsonwebtoken": "^9.0.2",
    "dotenv": "^16.3.1",
    "node-telegram-bot-api": "^0.64.0",
    "uuid": "^9.0.1",
    "joi": "^17.11.0",
    "moment": "^2.29.4",
    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "jest": "^29.7.0",
    "supertest": "^6.3.3",
    "eslint": "^8.54.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.1.1",
    "@types/jest": "^29.5.8",
    "cross-env": "^7.0.3"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/EternalDigitalAssets/KYCACCREDEFI.git"
  },
  "bugs": {
    "url": "https://github.com/EternalDigitalAssets/KYCACCREDEFI/issues"
  },
  "homepage": "https://github.com/EternalDigitalAssets/KYCACCREDEFI#readme",
  "jest": {
    "testEnvironment": "node",
    "collectCoverageFrom": [
      "*.js",
      "!node_modules/**",
      "!coverage/**",
      "!jest.config.js"
    ],
    "coverageDirectory": "coverage",
    "coverageReporters": [
      "text",
      "lcov",
      "html"
    ]
  },
  "eslintConfig": {
    "extends": [
      "standard"
    ],
    "env": {
      "node": true,
      "jest": true
    },
    "rules": {
      "no-console": "warn",
      "prefer-const": "error",
      "no-unused-vars": "error"
    }
  }
}

